<div class="notes_content">
	All Notes
	<% @notes.each do |note|%>
		<div class="notes_item">
    		<p><a href="#" value="/notes/<%= note.id%>" class="notes_a"> <%= note.title %></a><p>
    	</div>
	<% end %>
</div>

<script>
	$(".notes_a").on("click", function(){
		var url = $(this).attr("value");
		coloboxHTML(url);
	});
</script>