<a href='#' onclick="updateNote('<%= @note.id%>', $('#note_body').val())">Save</a>
<a href='#' onclick="deleteNote('<%= @note.id%>')">Delete</a>

<br /><br />
<h4><%= @note.title.capitalize %></h4>

<fieldset>
	<br />
	<center>
		<div>
			<textarea id="note_body" rows=12 cols=60><%= @note.unsafe_body %></textarea>
		</div>
	</center>
</fieldset>