<%
	note = @new_note if @new_note
%>

<tr id="note_line-<%= note.id%>" >
  <td style="cursor: pointer" id="note_item-<%= note.id%>" onclick="showNote(this)" value="<%= note_path(note) %>"><%= note.title %></td>
  <td id="note_category-<%= note.id%>" ><%= note.category_title %></td>
  <td><%= note.formatted_created_At %></td>
</tr>